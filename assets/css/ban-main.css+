.timeline ul li {
  list-style-type: none;
  position: relative;
  width: 2px;
  margin: 0 auto;
  padding-top: 50px;
  background: #5BABBF; }
  .timeline ul li .timeline--bullet {
    content: '';
    position: absolute;
    left: 50% !important;
